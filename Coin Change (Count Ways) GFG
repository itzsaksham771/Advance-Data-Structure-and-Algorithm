class Solution {
    public int count(int coins[], int sum) {
        // code here.
        int dp[] = new int[sum+1];
        dp[0] = 1;
        for(int coin : coins) {
            for(int s = coin; s<=sum; s++) {
                dp[s] += dp[s-coin];
            }
        }
        return dp[sum];
    }
}
